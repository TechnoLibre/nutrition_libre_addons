<odoo>
    <data>
        <template id="snippet_growth_calculator_category" inherit_id="website.snippets" name="Growth Calculator Snippet Category">
            <xpath expr="//div[@id='snippet_feature']//t[@t-snippet][last()]" position="after">
                <t t-snippet="snippet_growth_calculator.snippet_growth_calculator" string="Growth Calculator" t-thumbnail="/snippet_growth_calculator/static/src/images/icon.jpg">
                    <keywords>growth, calculator</keywords>
                </t>
            </xpath>
        </template>
    </data>
</odoo>

